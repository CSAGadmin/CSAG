<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <!-- Custom Portal Template Content -->
    <template id="ticket_submit_gab" inherit_id="website_helpdesk_form.ticket_submit" name="Helpdesk: Submit a Ticket GAB">

        <!-- Replace Content -->
        <xpath expr="//h2" position="replace">

            <div class="container mt-4">
                <h1 t-if="team">
                    <span><t t-esc="team.name"/></span>
                </h1>
            </div>

        </xpath>

    </template>

</odoo>